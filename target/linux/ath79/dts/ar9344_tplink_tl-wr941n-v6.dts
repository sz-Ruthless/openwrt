#include "ar9344.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "TP-Link TL-WR941N v6";
	compatible = "tplink,tl-wr941n-v6", "qca,ar9344";

	aliases {
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-running = &led_system;
		led-upgrade = &led_system;
		label-mac-device = &wmac;
	};

	leds: leds {
		compatible = "gpio-leds";

		led_system: system {
			label = "green:system";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		qss {
			label = "green:qss";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};
/*
		wlan {
			label = "green:wlan";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
*/
	};

	ath9k-leds {
		compatible = "gpio-leds";

		wlan {
			label = "green:wlan";
			gpios = <&ath9k 0 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};
	};
};

&spi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;
		m25p,fast-read;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x020000>;
			/*	read-only;	*/

				compatible = "nvmem-cells";
				#address-cells = <1>;
				#size-cells = <1>;

				macaddr_uboot: macaddr@1fc00 {
					reg = <0x1fc00 0x6>;
				};
			};

			partition@20000 {
				compatible = "tplink,firmware";
				label = "firmware";
				reg = <0x020000 0x7d0000>;
			};

			art: partition@7f0000 {
				label = "art";
				reg = <0x7f0000 0x010000>;
			/*	read-only;	*/
			};
		};
	};
};

&pinmux {
	pmx_led_wan_lan: pinmux_led_wan_lan {
		pinctrl-single,bits = <0x10 0x2c2d0000 0xffff0000>,
			<0x14 0x292a2b 0xffffff>;
	};
};

&builtin_switch {
	pinctrl-names = "default";
	pinctrl-0 = <&pmx_led_wan_lan>;
};

&pcie {
	status = "okay";

	ath9k: wifi@0,0 {
		reg = <0x0000 0 0 0 0>;
		gpio-controller;
		#gpio-cells = <2>;
		qca,no-eeprom;
		nvmem-cells = <&macaddr_uboot>;
		nvmem-cell-names = "mac-address";
	};
};

&eth1 {
	status = "okay";

	/* Workaround: keep the Ethernet interfaces order/mapping correct
	 * (GMAC0 -> eth0, GMAC1 -> eth1, same as in old ar71xx target) */
	compatible = "qca,ar9340-eth", "syscon", "simple-mfd";

	nvmem-cells = <&macaddr_uboot>;
	nvmem-cell-names = "mac-address";
	

	gmac-config {
		device = <&gmac>;
		switch-phy-swap = <0>;
		switch-only-mode = <1>;
	};
};

&eth0 {
	status = "okay";

	phy-handle = <&swphy4>;

	nvmem-cells = <&macaddr_uboot>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <1>;
};
